{
	"name": "php-class-creator",
	"displayName": "php class creator/generator",
	"description": "Help to create/generate php class file",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"publisher": "erlangparasu",
	"homepage": "https://github.com/erlangparasu/php-class-creator",
	"repository": {
		"type": "git",
		"url": "https://github.com/erlangparasu/php-class-creator.git"
	},
	"license": "Apache-2.0",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:php-class-creator.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "php-class-creator.helloWorld",
				"title": "Create PHP Class File"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder || resourceExtname == .php",
					"command": "php-class-creator.helloWorld",
					"alt": "markdown.showPreviewToSide",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}